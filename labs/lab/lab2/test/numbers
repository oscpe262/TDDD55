0           // INTEGER '0'
123         // INTEGER '123'

123.        // REAL '123.'
.123        // REAL '.123'
123.123     // REAL '123.123'

1.1e21      // REAL '1.1e21'

1.1E21      // REAL '1.1E21'
1.1E-21     // REAL '1.1-E21'
1.1E+21     // REAL '1.1+E21'

1.E21       // REAL '1.E21'
1.E-21      // REAL '1.E-21'
1.E+21      // REAL '1.E+21'

.1E21       // REAL '.1E21'
.1E-21      // REAL '.1E-21'
.1E+21      // REAL '.1E+21'

1E20        // REAL '1E20'
1E-20       // REAL '1E-20'
1E+20       // REAL '1E+20'

.           // '.'
